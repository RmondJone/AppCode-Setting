<templateSet group="ReactNavigation">
  <template name="navigationOptions" value="navigationOptions={&#10;    $a$&#10;}" description="配置StackNavigator的一些属性" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="title" value="title:&quot;$a$&quot;" description="标题，如果设置了这个导航栏和标签栏的title就会变成一样的，不推荐使用" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="header" value="header:&quot;$a$&quot;" description="可以设置一些导航的属性，如果隐藏顶部导航栏只要将这个属性设置为null" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="headerTitle" value="headerTitle:&quot;$a$&quot;" description="设置导航栏标题，推荐" toReformat="false" toShortenFQNames="true">
