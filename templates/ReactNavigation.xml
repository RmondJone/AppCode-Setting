      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="headerBackTitleStyle" value="headerBackTitleStyle:&quot;$a$&quot;" description="设置导航栏‘返回’文字样式" toReformat="false" toShortenFQNames="true">
