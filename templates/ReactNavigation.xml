  </template>
  <template name="headerTruncatedBackTitle" value="headerTruncatedBackTitle:&quot;$a$&quot;" description="设置当上个页面标题不符合返回箭头后的文字时，默认改成&quot;返回&quot;" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
