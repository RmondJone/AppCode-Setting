      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="headerRight" value="headerRight:&quot;$a$&quot;" description="设置导航条右侧。可以是按钮或者其他视图控件" toReformat="false" toShortenFQNames="true">
